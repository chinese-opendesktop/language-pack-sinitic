msgstr ""

#: C/genius.xml:5451(para)
msgid "Delete a column of a matrix."
msgstr ""

#: C/genius.xml:5456(term)
msgid "<anchor id=\"gel-function-DeleteRow\"/>DeleteRow"
msgstr ""

#: C/genius.xml:5458(synopsis)
#, no-wrap
