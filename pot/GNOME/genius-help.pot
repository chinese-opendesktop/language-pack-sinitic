#: C/genius.xml:5543(synopsis)
#, no-wrap
msgid "IsDiagonal (M)"
msgstr ""

#: C/genius.xml:5544(para)
msgid "Is a matrix diagonal."
msgstr ""

#: C/genius.xml:5545(para) C/genius.xml:5695(para)
msgid "See <ulink url=\"https://en.wikipedia.org/wiki/Diagonal_matrix\">Wikipedia</ulink> or <ulink url=\"http://planetmath.org/DiagonalMatrix\">Planetmath</ulink> for more information."
msgstr ""

#: C/genius.xml:5554(term)
msgid "<anchor id=\"gel-function-IsIdentity\"/>IsIdentity"
msgstr ""

#: C/genius.xml:5556(synopsis)
#, no-wrap
msgid "IsIdentity (x)"
msgstr ""

