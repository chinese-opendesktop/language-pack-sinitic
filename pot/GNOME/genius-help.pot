msgid "<anchor id=\"gel-function-StripZeroRows\"/>StripZeroRows"
msgstr ""

#: C/genius.xml:5863(synopsis)
#, no-wrap
msgid "StripZeroRows (M)"
msgstr ""

#: C/genius.xml:5864(para)
